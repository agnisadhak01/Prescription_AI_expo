    "scheme": "prescription-ai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "agnisadhak",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ausomemgr.prescription",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.232795038046-rld1dn9s7ocnt93ouec71s27p9ir4pco"
