    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ausomemgr.prescription",
      "googleServicesFile": "./google-services.json",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "config": {
        "googleSignIn": {
          "apiKey": "AIzaSyAIXYB9LWfMN6KQfXWnlx_rZr9vNrKbLrc",
          "certificateHash": "ABF15BE5666196500666777FE27CD0709366A01CD",
          "productionCertificateHash": "8F0F160335E1EA51296F64FFFE8F4B24C95D645F"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "prescription-ai"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "versionCode": 8
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
